page {
  background-color: #ededed;
}

swiper {
  width: 100%;
  height: 300rpx;
}

swiper image {
  width: 100%;
  height: 300rpx;
}

.title {
  height: 80rpx;
  text-align: center;
  font-size: 26rpx;
  color: #666;
}

.title text {
  line-height: 80rpx;
}

.family {
  float: left;
  width: 225rpx;
  height: 285rpx;
  margin: 12rpx;
  text-align: center;
}

.family image {
  width: 200rpx;
  height: 200rpx;
  border: 1px solid #666666;
  border-radius: 50%;
}

.family text {
  margin-top: 10rpx;
  font-size: 28rpx;
}